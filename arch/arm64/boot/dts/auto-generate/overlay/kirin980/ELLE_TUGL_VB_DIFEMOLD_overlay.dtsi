		huawei,cam-power-down-seq-cfg-val = <0 0 1140000 2800000 1800000>;
		huawei,cam-power-down-seq-val = <0xFF 0xFF 0xFF 3 0xFF>;
		huawei,cam-power-down-seq-sensor-index = <0xFF 1 0xFF 0xFF 0xFF>;
